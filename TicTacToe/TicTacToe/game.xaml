<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TicTacToe.game">
        <Grid BackgroundColor="CadetBlue" Padding="20">
            <Button Grid.Column="0" Grid.Row="0" x:Name="b1" Clicked="Checked" ></Button>
            <Button Grid.Column="0" Grid.Row="1" x:Name="b2" Clicked="Checked" ></Button>
            <Button Grid.Column="0" Grid.Row="2" x:Name="b3" Clicked="Checked" ></Button>
            <Button Grid.Column="1" Grid.Row="0" x:Name="b4" Clicked="Checked" ></Button>
            <Button Grid.Column="1" Grid.Row="1" x:Name="b5" Clicked="Checked" ></Button>
            <Button Grid.Column="1" Grid.Row="2" x:Name="b6" Clicked="Checked" ></Button>
            <Button Grid.Column="2" Grid.Row="0" x:Name="b7" Clicked="Checked" ></Button>
            <Button Grid.Column="2" Grid.Row="1" x:Name="b8" Clicked="Checked" ></Button>
            <Button Grid.Column="2" Grid.Row="2" x:Name="b9" Clicked="Checked" ></Button>
    </Grid>
</ContentPage>